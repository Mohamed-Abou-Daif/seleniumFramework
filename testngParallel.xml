<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite  name="Regrtion Suite" parallel="tests" thread-count="2">

  <test name="Chrome Regression Test">
  <parameter name="browser" value="chrome"/>
    <classes>
      <class name="tests.UserRegistrationParallelTesting"/>  
    </classes>
  </test> 
  
  <test name="Firfox Regression Test">
  <parameter name="browser" value="firefox"/>
    <classes>
      <class name="tests.MyAccountParallelTest"/>  
    </classes>
  </test> 
</suite> 
